@model PartialViewsVM

<div class="row justify-content-center">
    <div class="text-center">
        <!-- Header Menu begins -->
        <div class="@(ViewData["Title"]?.ToString() == "New Trade" ? "" : "card-header")">
            <div class="row align-items-center">
                <!-- Header tabs begin -->
                <div class="col mr-md-5 d-flex justify-content-center">
                    <div id="researchDataTabHeaders">
                        <ul class="nav nav-tabs" id="journalTabs" role="tablist">
                            <li class="nav-item" role="presentation">
                                <button class="nav-link active" id="pnl-tab" data-toggle="tab" data-target="#pnl" type="button" role="tab" aria-controls="pnl" aria-selected="true">PnL</button>
                            </li>
                            <li class="nav-item">
                                <button class="nav-link" id="entry-tab" data-toggle="tab" data-target="#entry" type="button" role="tab" aria-controls="entry" aria-selected="false">Entry</button>
                            </li>
                            <li class="nav-item">
                                <button class="nav-link" id="priceActionContext-tab" data-toggle="tab" data-target="#priceActionContext" type="button" role="tab" aria-controls="priceActionContext" aria-selected="false">PA Context</button>
                            </li>
                            <li class="nav-item">
                                <button class="nav-link" id="fullATR-tab" data-toggle="tab" data-target="#fullATR" type="button" role="tab" aria-controls="fullATR" aria-selected="false">Full ATR</button>
                            </li>
                        </ul>
                    </div>
                </div>
                <!-- Header tabs ends -->
            </div>
        </div>
        <!-- Header Menu ends -->
        <!-- Card body begins -->
        <div class="card-body" id="cardBody">
            <div class="tab-content">
                <!-- Tab content PnL -->
                <div class="tab-pane fade show active tundora text-left" id="pnl" role="tabpanel" aria-labelledby="pnl-tab">
                    <div class="row">
                        <div class="col">
                            <div class="d-flex flex-wrap">
                                <div class="form-group col-12 col-md-6 col-lg">
                                    <label class="d-block text-nowrap min-width-60" for="TradeRatingInput">Rating</label>
                                    <select class="text-center form-control" id="TradeRatingInput" data-research="TradeRatingDisplay" asp-for="@Model.ResearchFirstBarPullbackDisplay.TradeRatingDisplay" asp-items="Model.TradeRating"></select>
                                </div>
                                <div class="form-group col-12 col-md-6 col-lg">
                                    <label class="d-block text-nowrap min-width-60" for="OneToOneHitOnInput">1:1 hit on</label>
                                    <input class="text-center form-control" id="OneToOneHitOnInput" data-research="OneToOneHitOnDisplay" value="@Model.ResearchFirstBarPullbackDisplay.OneToOneHitOnDisplay" />
                                </div>
                                <div class="form-group col-12 col-md-6 col-lg">
                                    <label class="d-block text-nowrap min-width-60" for="IsOneToThreeHitInput">1:3 hit</label>
                                    <select class="text-center form-control" id="IsOneToThreeHitInput" data-research="IsOneToThreeHitDisplay" asp-for="@Model.ResearchFirstBarPullbackDisplay.IsOneToThreeHitDisplay" asp-items="Model.YesNoOptions"></select>
                                </div>
                                <div class="form-group col-12 col-md-6 col-lg">
                                    <label class="d-block text-nowrap min-width-60" for="IsOneToFiveHitInput"> 1:5 hit</label>
                                    <select class="text-center form-control" id="IsOneToFiveHitInput" data-research="IsOneToFiveHitDisplay" asp-for="@Model.ResearchFirstBarPullbackDisplay.IsOneToFiveHitDisplay" asp-items="Model.YesNoOptions"></select>
                                </div>
                                <div class="form-group col-12 col-md-6 col-lg">
                                    <label class="d-block text-nowrap min-width-60" for="IsBreakevenInput">Breakeven</label>
                                    <select class="text-center form-control" id="IsBreakevenInput" data-research="IsBreakevenDisplay" asp-for="@Model.ResearchFirstBarPullbackDisplay.IsBreakevenDisplay" asp-items="Model.YesNoOptions"></select>
                                </div>
                                <div class="form-group col-12 col-md-6 col-lg">
                                    <label class="d-block text-nowrap min-width-60" for="IsLossInput">Loss</label>
                                    <select class="text-center form-control" id="IsLossInput" data-research="IsLossDisplay" asp-for="@Model.ResearchFirstBarPullbackDisplay.IsLossDisplay" asp-items="Model.YesNoOptions"></select>
                                </div>
                                <div class="form-group col-12 col-md-6 col-lg">
                                    <label class="d-block text-nowrap min-width-60" for="MaxRRInput">Max RR</label>
                                    <input class="text-center form-control" id="MaxRRInput" data-research="MaxRRDisplay" value="@Model.ResearchFirstBarPullbackDisplay.MaxRRDisplay" />
                                </div>
                                <div class="form-group col-12 col-md-6 col-lg">
                                    <label class="d-block text-nowrap min-width-60" for="MarketGaveSmthInput">Market gave smth</label>
                                    <select class="text-center form-control" id="MarketGaveSmthInput" data-research="MarketGaveSmthDisplay" asp-for="@Model.ResearchFirstBarPullbackDisplay.MarketGaveSmthDisplay" asp-items="Model.YesNoOptions"></select>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row mt-2">
                        <div class="col">
                            <div class="form-group col-12 col-md-6 col-lg">
                                <label class="d-block text-nowrap min-width-60" for="Comment">Comment</label>
                                <input class="text-justify form-control" id="Comment" data-research="CommentDisplay" value="@Model.ResearchFirstBarPullbackDisplay.CommentDisplay" />
                            </div>

                        </div>
                    </div>
                </div>
                <!-- Tab content Entry -->
                <div class="tab-pane fade show tundora text-left" id="entry" role="tabpanel" aria-labelledby="entry-tab">
                    <div class="row">
                        <div class="col">
                            <div class="d-flex flex-wrap">
                                <div class="form-group col-12 col-md-6 col-lg">
                                    <label class="d-block text-nowrap min-width-60" for="IsEntryAfteriBarDisplayInput">Entry after i bar</label>
                                    <select class="text-center form-control" id="IsEntryAfteriBarDisplayInput" data-research="IsEntryAfteriBarDisplay" asp-for="@Model.ResearchFirstBarPullbackDisplay.IsEntryAfteriBarDisplay" asp-items="Model.YesNoOptions"></select>
                                </div>
                                <div class="form-group col-12 col-md-6 col-lg">
                                    <label class="d-block text-nowrap min-width-60" for="IsSignalBarInTradeDirectionInput">Signal bar in my direction</label>
                                    <select class="text-center form-control" id="IsSignalBarInTradeDirectionInput" data-research="IsSignalBarInTradeDirectionDisplay" asp-for="@Model.ResearchFirstBarPullbackDisplay.IsSignalBarInTradeDirectionDisplay" asp-items="Model.YesNoOptions"></select>
                                </div>
                                <div class="form-group col-12 col-md-6 col-lg">
                                    <label class="d-block text-nowrap min-width-60" for="IsEntryAfter3To5BarsInput">Entry at 3-5 bars</label>
                                    <select class="text-center form-control" id="IsEntryAfter3To5BarsInput" data-research="IsEntryAfter3To5BarsDisplay" asp-for="@Model.ResearchFirstBarPullbackDisplay.IsEntryAfter3To5BarsDisplay" asp-items="Model.YesNoOptions"></select>
                                </div>
                                <div class="form-group col-12 col-md-6 col-lg">
                                    <label class="d-block text-nowrap min-width-60" for="IsEntryAfter5BarsInput">Entry after 5 bars</label>
                                    <select class="text-center form-control" id="IsEntryAfter5BarsInput" data-research="IsEntryAfter5BarsDisplay" asp-for="@Model.ResearchFirstBarPullbackDisplay.IsEntryAfter5BarsDisplay" asp-items="Model.YesNoOptions"></select>
                                </div>
                                <div class="form-group col-12 col-md-6 col-lg">
                                    <label class="d-block text-nowrap min-width-60" for="IsEntryBeforePreviousSwingOnTriggerDisplay">Entry before prev swing on trigger</label>
                                    <select class="text-center form-control" id="IsEntryBeforePreviousSwingOnTriggerInput" data-research="IsEntryBeforePreviousSwingOnTriggerDisplay" asp-for="@Model.ResearchFirstBarPullbackDisplay.IsEntryBeforePreviousSwingOnTriggerDisplay" asp-items="Model.YesNoOptions"></select>
                                </div>
                                <div class="form-group col-12 col-md-6 col-lg">
                                    <label class="d-block text-nowrap min-width-60" for="IsEntryAtPreviousSwingOnTriggerInput">Entry at prev swing on trigger</label>
                                    <select class="text-center form-control" id="IsEntryAtPreviousSwingOnTriggerInput" data-research="IsEntryAtPreviousSwingOnTriggerDisplay" asp-for="@Model.ResearchFirstBarPullbackDisplay.IsEntryAtPreviousSwingOnTriggerDisplay" asp-items="Model.YesNoOptions"></select>
                                </div>
                                <div class="form-group col-12 col-md-6 col-lg">
                                    <label class="d-block text-nowrap min-width-60" for="IsEntryBeforePreviousSwingOn4HInput">Entry before prev swing on 4H</label>
                                    <select class="text-center form-control" id="IsEntryBeforePreviousSwingOn4HInput" data-research="IsEntryBeforePreviousSwingOn4HDisplay" asp-for="@Model.ResearchFirstBarPullbackDisplay.IsEntryBeforePreviousSwingOn4HDisplay" asp-items="Model.YesNoOptions"></select>
                                </div>
                                <div class="form-group col-12 col-md-6 col-lg">
                                    <label class="d-block text-nowrap min-width-60" for="IsEntryBeforePreviousSwingOnDInput">Entry before prev swing on D</label>
                                    <select class="text-center form-control" id="IsEntryBeforePreviousSwingOnDInput" data-research="IsEntryBeforePreviousSwingOnDDisplay" asp-for="@Model.ResearchFirstBarPullbackDisplay.IsEntryBeforePreviousSwingOnDDisplay" asp-items="Model.YesNoOptions"></select>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- Tab content Price Aaction Context -->
                <div class="tab-pane fade show tundora text-left" id="priceActionContext" role="tabpanel" aria-labelledby="priceActionContext-tab">
                    <div class="row">
                        <div class="col">
                            <div class="d-flex flex-wrap">
                                <div class="form-group col-12 col-md-6 col-lg">
                                    <label class="d-block text-nowrap min-width-60" for="IsMomentumTradeInput">Momentum trade</label>
                                    <select class="text-center form-control" id="IsMomentumTradeInput" data-research="IsMomentumTradeDisplay" asp-for="@Model.ResearchFirstBarPullbackDisplay.IsMomentumTradeDisplay" asp-items="Model.YesNoOptions"></select>
                                </div>
                                <div class="form-group col-12 col-md-6 col-lg">
                                    <label class="d-block text-nowrap min-width-60" for="IIsTrendTradeInput">Trend trade</label>
                                    <select class="text-center form-control" id="IsTrendTradeInput" data-research="IsTrendTradeDisplay" asp-for="@Model.ResearchFirstBarPullbackDisplay.IsTrendTradeDisplay" asp-items="Model.YesNoOptions"></select>
                                </div>
                                <div class="form-group col-12 col-md-6 col-lg">
                                    <label class="d-block text-nowrap min-width-60" for="IsTriggerTrendingInput">Trigger trending</label>
                                    <select class="text-center form-control" id="IsTriggerTrendingInput" data-research="IsTriggerTrendingDisplay" asp-for="@Model.ResearchFirstBarPullbackDisplay.IsTriggerTrendingDisplay" asp-items="Model.YesNoOptions"></select>
                                </div>
                                <div class="form-group col-12 col-md-6 col-lg">
                                    <label class="d-block text-nowrap min-width-60" for="Is4HTrendingInput">4H trending</label>
                                    <select class="text-center form-control" id="Is4HTrendingInput" data-research="Is4HTrendingDisplay" asp-for="@Model.ResearchFirstBarPullbackDisplay.Is4HTrendingDisplay" asp-items="Model.YesNoOptions"></select>
                                </div>
                                <div class="form-group col-12 col-md-6 col-lg">
                                    <label class="d-block text-nowrap min-width-60" for="IsDTrendingInput">D trending</label>
                                    <select class="text-center form-control" id="IsDTrendingInput" data-research="IsDTrendingDisplay" asp-for="@Model.ResearchFirstBarPullbackDisplay.IsDTrendingDisplay" asp-items="Model.YesNoOptions"></select>
                                </div>
                                <div class="form-group col-12 col-md-6 col-lg">
                                    <label class="d-block text-nowrap min-width-60" for="IsSignalBarStrongReversalDisplayInput">Signal bar strong reversal</label>
                                    <select class="text-center form-control" id="IsSignalBarStrongReversalDisplayInput" data-research="IsSignalBarStrongReversalDisplay" asp-for="@Model.ResearchFirstBarPullbackDisplay.IsSignalBarStrongReversalDisplay" asp-items="Model.YesNoOptions"></select>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- Tab content Full ATR -->
                <div class="tab-pane fade show tundora text-left" id="fullATR" role="tabpanel" aria-labelledby="fullATR-tab">
                    <div class="row">
                        <div class="col">
                            <div class="d-flex flex-wrap">
                                <div class="form-group col-12 col-md-6 col-lg">
                                    <label class="d-block text-nowrap min-width-60" for="FullATROneToOneHitOn">1:1 hit on</label>
                                    <input class="text-center form-control" id="FullATROneToOneHitOn" data-research="FullATROneToOneHitOnDisplay" value="@Model.ResearchFirstBarPullbackDisplay.FullATROneToOneHitOnDisplay" />
                                </div>
                                <div class="form-group col-12 col-md-6 col-lg">
                                    <label class="d-block text-nowrap min-width-60" for="IsFullATROneToThreeHit">1:3 hit</label>
                                    <select class="text-center form-control" id="IsFullATROneToThreeHit" data-research="IsFullATROneToThreeHitDisplay" asp-for="@Model.ResearchFirstBarPullbackDisplay.IsFullATROneToThreeHitDisplay" asp-items="Model.YesNoOptions"></select>
                                </div>
                                <div class="form-group col-12 col-md-6 col-lg">
                                    <label class="d-block text-nowrap min-width-60" for="IsFullATROneToFiveHit"> 1:5 hit</label>
                                    <select class="text-center form-control" id="IsFullATROneToFiveHit" data-research="IsFullATROneToFiveHitDisplay" asp-for="@Model.ResearchFirstBarPullbackDisplay.IsFullATROneToFiveHitDisplay" asp-items="Model.YesNoOptions"></select>
                                </div>
                                <div class="form-group col-12 col-md-6 col-lg">
                                    <label class="d-block text-nowrap min-width-60" for="IsFullATRBreakeven">Breakeven</label>
                                    <select class="text-center form-control" id="IsFullATRBreakeven" data-research="IsFullATRBreakevenDisplay" asp-for="@Model.ResearchFirstBarPullbackDisplay.IsFullATRBreakevenDisplay" asp-items="Model.YesNoOptions"></select>
                                </div>
                                <div class="form-group col-12 col-md-6 col-lg">
                                    <label class="d-block text-nowrap min-width-60" for="IsFullATRLoss">Loss</label>
                                    <select class="text-center form-control" id="IsFullATRLoss" data-research="IsFullATRLossDisplay" asp-for="@Model.ResearchFirstBarPullbackDisplay.IsFullATRLossDisplay" asp-items="Model.YesNoOptions"></select>
                                </div>
                                <div class="form-group col-12 col-md-6 col-lg">
                                    <label class="d-block text-nowrap min-width-60" for="FullATRMaxRR">Max RR</label>
                                    <input class="text-center form-control" id="FullATRMaxRR" data-research="FullATRMaxRRDisplay" value="@Model.ResearchFirstBarPullbackDisplay.FullATRMaxRRDisplay" />
                                </div>
                                <div class="form-group col-12 col-md-6 col-lg">
                                    <label class="d-block text-nowrap min-width-60" for="FullATRMarketGaveSmth">Market gave smth</label>
                                    <select class="text-center form-control" id="FullATRMarketGaveSmth" data-research="FullATRMarketGaveSmthDisplay" asp-for="@Model.ResearchFirstBarPullbackDisplay.FullATRMarketGaveSmthDisplay" asp-items="Model.YesNoOptions"></select>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>